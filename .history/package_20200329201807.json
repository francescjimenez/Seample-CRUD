{
  "name": "order_explorer",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "start": "npm run build:client && npm run start:server",
    "dev": "npm run start:client && npm run start:dev",
    "start:client": "react-app-rewired start",
    "start:server": "node server/index.js",
    "build:client": "react-app-rewired build",
    "dev:server": "nodemon server/index.js",
    "test": "npm run test:client",
    "test:client": "react-app-rewired test --env=jsdom",
    "docs": "apidoc -i server -o docs && apidoc-postman -i server -o docs/postman",
    "docs-postman": "apidoc-postman -i server -o docs"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "socket.io": "2.3.0",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "ddos": "0.2.1",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-server-logs": "1.0.4",
    "express-validation": "1.0.3",
    "http-status": "1.4.2",
    "joi": "14.3.1",
    "helmet": "3.22.0",
    "bootstrap": "4.4.1",
    "mongoose": "5.8.7",
    "node-schedule": "^1.3.2",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-bootstrap": "1.0.0",
    "react-bootstrap-table": "4.3.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.4.1"
  },
  "devDependencies": {
    "apidoc": "0.20.1",
    "apidoc-postman": "0.0.92",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "3.4.4",
    "eslint": "6.8.0",
    "eslint-plugin-react": "7.19.0",
    "pre-commit": "1.2.2",
    "react-app-rewired": "2.1.5",
    "nodemon": "2.0.2"
  },
}
